sLt2M = file.size / 1024 / 1024 < 2;\n      if (!isJPG) {\n        this.$message.error('上传头像只能是图片格式!');\n        return false;\n      }\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n        return false;\n      }\n      return isJPG && isLt2M;\n    },\n    // 提交修改的表单数据\n    async editChairmans() {\n      const {data: rese} = await this.$http.post('/chairman/editChairmans', this.editForm);\n    },\n\n\n  }\n\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}